  <code>show_debug_overlay(true);
audio_stop_all();
global.vol_music=0.5;
global.vol_fx=0.66;
global.current_world=0;
//set up arrays
var a1=9999;
var a2=2;
do
{
    global.crawl_square_array[a1,a2]=-1
    a2-=1
    if a2&lt;0
    {   
        a2=2;
        a1-=1;
    }
}
until a1=-1;

var a1=20;
var a2=10;
do
{
    global.crawl_room_array[a1,a2]=-1
    a2-=1
    if a2&lt;0
    {   
        a2=10;
        a1-=1;
    }
}
until a1=-1;

//vars 

global.crawl_player_position=0;
global.crawl_player_angle=0;

scr_crawle_create();

//setting actors states

enum player_crawl_state
{
    init,
    turn,
    moving,
    drawing,
    choose_item,
    choose_health_potion,
    taking_damage,
    waiting,
    transition,
    off
};

//setting view based on screen
display_w = round(window_get_width());
display_h = round(window_get_height());
display_aspect_ratio = display_w/display_h;

view_hview[0] = view_wview[0] / display_aspect_ratio;
view_hport[0] = view_wview[0] / display_aspect_ratio;

//creating gui elements
instance_create(0,0,obj_crawl_gui);
global.difficult=2;

//music load
global.change_music=0;
audio_group_load(odfm1);
audio_group_load(odfm2);
audio_group_load(odfm3);
audio_group_load(odfm4);
audio_group_load(odfm5);
audio_group_load(odfm6);
audio_group_load(title_and_menu);
audio_group_load(boss_music_1);</code>
