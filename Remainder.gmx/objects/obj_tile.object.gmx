        p_array[a1]=instance_create(x+global.point_array[a2,7]*7.8,y+global.point_array[a2,8]*7.8,obj_point);
        p_array[a1].type=global.point_array[a2,0];
        p_array[a1].used=global.point_array[a2,2];
        p_array[a1].point_n=a2;
        a1-=1
        a2-=1
    }until a1=-1
    //set image_index
    if image&lt;3
    {
        sprite_index=scr_scroll_tile_top;
        image_index=image;
    }
    else
    {
        if image&lt;7
        {
            sprite_index=scr_scroll_tile_mid;
            image_index=image-3;
            
        }
        else
        {
            sprite_index=scr_scroll_tile_bottom;
            image_index=image-7;
        }
    }
    
    //geo fx
    if  !(geo=-1)&amp;&amp;geo_gate=1 {geo_fx=instance_create(x,y,obj_tile_geo);geo_gate=0};
    if !(geo_fx=-1) geo_fx.type=geo;
    //set burned
    if tile_n&lt;global.burn_tile
    {
        burn_state=1

        var a1=point_n-1
        do
        {
            p_array[a1].sprite_index=spr_empty;
            a1-=1;
        }
        until a1=-1;
        if !(geo_fx=-1) geo_fx.sprite_index=spr_empty;
        sprite_index=spr_empty;
    }
    //deco fx
    if !deco=0
    {
        deco_fx=instance_create(x,y,obj_tile_deco);
        deco_fx.sprite_index=deco;
    }
    init=0;
}
