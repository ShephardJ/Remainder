    //change varient
    if(varient=0)&amp;&amp;(damage_track_result=1)
    {
        if (damage_taken_type_1=0) {varient=2;start_up=1;}
        if (damage_taken_type_1=1) {varient=1;start_up=1;}
        if (damage_taken_type_1=4) {varient=3;start_up=1;}
    }
    //infection
    scr_zombie_infection();
    
    //set pause
    waiting=1;
    hit=0;
    zombie_hit_sound=audio_play_sound(snd_zombie_hit,5,false);
    audio_sound_gain(zombie_hit_sound,global.vol_fx*0.75,0);
    //damage reset
    damage_taken_type_1=0;damage_taken_1=0;amage_taken_type_2=0;damage_taken_2=0;
}
////////////////////////////////////////////////////////////////////////////////////////////
