                doing_array_numb+=1;
        }
        until doing_array_numb=3;
        
    }
    else
    {
        audio_sound_gain(zombie_hit_sound,0,0);
        zombie_dead_sound=audio_play_sound(snd_zombie_death,5,false);
        audio_sound_gain(zombie_dead_sound,global.vol_fx*0.75,0);
        instance_destroy();
    }
}
    


