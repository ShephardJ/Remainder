            <string>///spell
if (spell=1)
{
    if(sprite_index=sprite_main)
    {
        sprite_index=sprite_spell
        image_index=1;
        image_speed=12/60
    }
    if(sprite_index=sprite_spell)
    {
        if image_index&gt;8&amp;&amp;spell_gate=1
        {
            var heal_countdown=14
            do
            {
                if heal_countdown&gt;=0
                {
                //set heal_x
                if heal_countdown&gt;4 var heal_x=473;
                else var heal_x=152;
                if heal_countdown&gt;9 var heal_x=793;
                
                //set heal_y
                var heal_checking=heal_countdown;
                if (heal_checking&gt;4)
                {
                    do
                    {
                        heal_checking-=5;
                    }
                    until heal_checking&lt;0;
                    heal_checking+=5;
                }
                var heal_y= 271 +240*heal_checking
                // check position for someone
                if (place_meeting(heal_x,heal_y+10,all))
                {
                    var spell_check=instance_place(heal_x,heal_y,all);
                    if!(spell_check.init_hp=spell_check.hp)&amp;&amp;(spell_check.alive=1)
                    {
                        heal=instance_create(heal_x+70,heal_y+70,obj_flash_heal);
                        heal.heal_amount=3
                    }
                }               
                
                //increment heal_countdown
                heal_countdown-=1;
                }
            }
            until heal_countdown&lt;0;
            spell_gate=0;  
        }
        if (image_index&gt;17)
        {
            sprite_index=sprite_main;
            image_index=1
            spell=0
            global.enemy_acting=0;
        }
        
    }
}
if global.enemy_turn=0 {start_spell=1;global.player_wait_wait=1;sprite_index=sprite_main;spell_gate=1;wait_lock_down=0;}
